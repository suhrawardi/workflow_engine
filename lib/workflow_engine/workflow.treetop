grammar Workflow
  rule document
    (step / if / concurrent / space)* <Document>
  end

  rule concurrent
    "concurrent" (document) "end" <Concurrent>
  end

  rule step
    "step" space [a-z_\.]+ <Step>
  end

  rule if
    "if" space expression <If>
  end

  rule expression
    "[" [a-z_\.]+ "]" <Expression>
  end

  rule space
    [\s]+
  end
end
